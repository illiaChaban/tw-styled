{
  "include": [
    "src/**/*",
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ESNext",
    "lib": [
      "es2019",
      "DOM"
    ],
    "jsx": "preserve",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsxImportSource": "solid-js"
  },
}